{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "jsx": "react",
    "esModuleInterop": true,
    "sourceMap": true,
    "baseUrl": ".",
    "strict": true,
    "paths": {
      "@/*": ["src/*"],
      "@/model": ["src/model"],
      "@/utils": ["src/utils"],
      "@/components": ["src/components"]
    },
    "allowSyntheticDefaultImports": true
  }
}
